    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../build_taint_cpp_Debug/linux/src/taint_main",
            "args": ["/tmp/tmp2iznAP/instrace_logs", "--binary", "self-checksumming/samples/check_input_all/test_small+flatten", "--output", "self-checksumming/samples/check_input_all/test_small+flatten_patched"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}